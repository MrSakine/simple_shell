gcc -Wall -Werror -Wextra -pedantic -std=gnu89 -g *.c -o hsh

1.
echo "/bin/ls
/bin/ls
/bin/ls" | valgrind --leak-check=full --show-leak-kinds=all --track-origins=yes ./hsh 
echo "/bin/ls
     /bin/ls
/bin/ls
   /bin/ls     " | valgrind --leak-check=full --show-leak-kinds=all --track-origins=yes ./hsh 
echo "/bin/ls" | valgrind --leak-check=full --show-leak-kinds=all --track-origins=yes ./hsh 
echo "                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       " | valgrind --leak-check=full --show-leak-kinds=all --track-origins=yes ./hsh 
echo "                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               " | valgrind --leak-check=full --show-leak-kinds=all --track-origins=yes ./hsh 
echo "                " | valgrind --leak-check=full --show-leak-kinds=all --track-origins=yes ./hsh 
echo "./hbtn_ls /var" | valgrind --leak-check=full --show-leak-kinds=all --track-origins=yes ./hsh 

2.
echo "/bin/ls /var" | valgrind --leak-check=full --show-leak-kinds=all --track-origins=yes ./hsh 
echo "/bin/ls -l" | valgrind --leak-check=full --show-leak-kinds=all --track-origins=yes ./hsh 
echo "/bin/echo Holberton" | valgrind --leak-check=full --show-leak-kinds=all --track-origins=yes ./hsh 

4.
echo "/bin/ls /test_hbtn
exit" | valgrind --leak-check=full --show-leak-kinds=all --track-origins=yes ./hsh 
echo "exit" | valgrind --leak-check=full --show-leak-kinds=all --track-origins=yes ./hsh 

6.
echo "                                       /bin/ls                                                                                                                                                                                                                                                                                                                                                                                                                                                     " | valgrind --leak-check=full --show-leak-kinds=all --track-origins=yes ./hsh
echo "exit" | valgrind --leak-check=full --show-leak-kinds=all --track-origins=yes ./hsh 

3.
echo "hbtn_cmd" | valgrind --leak-check=full --show-leak-kinds=all --track-origins=yes ./hsh 
echo "../hbtn_ls /var" | valgrind --leak-check=full --show-leak-kinds=all --track-origins=yes ./hsh 
echo "./.././../hbtn_ls /var" | valgrind --leak-check=full --show-leak-kinds=all --track-origins=yes ./hsh
echo "./.././../////././hbtn_ls /var" | valgrind --leak-check=full --show-leak-kinds=all --track-origins=yes ./hsh 
echo "../../hbtn_ls /var" | valgrind --leak-check=full --show-leak-kinds=all --track-origins=yes ./hsh
echo "hbtn_ls /var" | valgrind --leak-check=full --show-leak-kinds=all --track-origins=yes ./hsh
echo "hbtn_ls" | valgrind --leak-check=full --show-leak-kinds=all --track-origins=yes ./hsh
echo "hbtn" | valgrind --leak-check=full --show-leak-kinds=all --track-origins=yes ./hsh 
